                                <input type="number" name="fx10Comercial" id="fx10Comercial"
                                    class="form-control fxComercial" disabled>
                            </div>
                        </div>
                    </div>
                    <div class="row infoSpread">
                        <div class="col-sm-4">
                            <label for="ValorSpread" class="formllabel">
                                Valor de Spread
                            </label>
                        </div>
                        <div class="col-sm-4">
                            <div class="input-group">
                                <span class="input-group-text">(%)</span>
                                <input type="number" name="valorSpread" id="valorSpread" class="form-control valorSpread">
                            </div>
                        </div>
